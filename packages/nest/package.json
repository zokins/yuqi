{
  "name": "@yuqijs/nest",
  "version": "0.0.1",
  "private": false,
  "description": "Nest server integration for @yuqijs",
  "keywords": [
    "rpc",
    "rest",
    "typescript",
    "typescript-rest",
    "nestjs",
    "http",
    "api",
    "type safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zokins/yuqi.git",
    "directory": "packages/nest"
  },
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "rxjs": "^7.1.0",
    "zod": "^3.22.3"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/platform-express": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/platform-fastify": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@yuqijs/core": "workspace:*",
    "@yuqijs/eslint-config": "workspace:*",
    "@yuqijs/prettier-config": "workspace:*",
    "@yuqijs/tsconfig": "workspace:*",
    "@yuqijs/vitest-config": "workspace:*",
    "eslint": "catalog:",
    "fastify": "^4.0.0",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@yuqijs/prettier-config"
}
