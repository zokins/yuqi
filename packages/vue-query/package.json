{
  "name": "@yuqijs/vue-query",
  "version": "0.0.1",
  "private": false,
  "description": "vue-query client integration for @yuqijs",
  "keywords": [
    "rpc",
    "rest",
    "typescript",
    "typescript-rest",
    "vue-query",
    "nestjs",
    "http",
    "api",
    "type safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zokins/yuqi.git",
    "directory": "packages/vue-query"
  },
  "license": "MIT",
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "peerDependencies": {
    "@tanstack/vue-query": "^4.0.0",
    "zod": "^3.22.3"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@yuqijs/core": "workspace:*",
    "@yuqijs/eslint-config": "workspace:*",
    "@yuqijs/prettier-config": "workspace:*",
    "@yuqijs/tsconfig": "workspace:*",
    "@yuqijs/vitest-config": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@yuqijs/prettier-config"
}
